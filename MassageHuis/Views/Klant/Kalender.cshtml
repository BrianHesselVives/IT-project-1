@using MassageHuis.ViewModels
@model MassageHuis.ViewModels.MasseurVM

<div class='container pb-5'>
    <h2>Plaats een afspraak bij: @Model.Naam</h2>
    <div class="list-group">
        <button type="button" class="list-group-item list-group-item-action active" aria-current="true">
            The current button
        </button>
        @foreach(DateTime vrijSlot in Model.vrijeSlots)
        {
            <button type="button" class="list-group-item list-group-item-action">@vrijSlot</button>
        }
    </div>
</div>

@await Html.PartialAsync("_KalenderLayout.cshtml")